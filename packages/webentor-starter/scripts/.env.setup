# 1Password vault ID
# IMPORTANT: Replace with your project's vault ID.
# Find it with: op vault list
OP_VAULT_ID=YOUR_OP_VAULT_ID

# 1Password item ID
# IMPORTANT: This is unique for each project.
# Find it with: op item get "Your .env item name" --format json | jq -r '.id'
OP_ITEM_ID=YOUR_OP_ITEM_ID

WP_THEMES="webentor-theme-v2"

# Setup runtime feature toggles.
# These values are project-owned and intentionally outside setup subtree updates.
SETUP_INTERACTIVE=true
SETUP_ENV_CHECK=true
SETUP_1PASSWORD=true
SETUP_COMPOSER=true
SETUP_THEME_DEPS=true
SETUP_WORDPRESS=true
SETUP_DB_SYNC=false
SETUP_SUBMODULES=false
SETUP_TYPESENSE=false
