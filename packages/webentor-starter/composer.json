{
  "name": "webikon/webentor-starter",
  "type": "project",
  "version": "2.0.2",
  "license": "MIT",
  "description": "WordPress boilerplate with Composer, easier configuration, and an improved folder structure",
  "homepage": "https://roots.io/bedrock/",
  "authors": [
    {
      "name": "Webikon s.r.o.",
      "email": "support@webikon.sk",
      "homepage": "https://webikon.sk"
    },
    {
      "name": "Pavol Caban",
      "email": "pavol.caban@webikon.sk",
      "homepage": "https://webikon.sk"
    }
  ],
  "keywords": [
    "bedrock",
    "composer",
    "roots",
    "wordpress",
    "wp",
    "wp-config"
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*",
        "wpackagist-theme/*"
      ]
    },
    {
      "type": "composer",
      "url": "https://composer.deliciousbrains.com"
    },
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "composer",
      "url": "https://connect.advancedcustomfields.com"
    },
    {
      "type": "package",
      "package": {
        "name": "gravityforms/gravityforms",
        "version": "2.8.21",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://www.gravityhelp.com/wp-content/plugins/gravitymanager/api.php?op=get_plugin&slug=gravityforms&key={%PLUGIN_GF_KEY}"
        },
        "require": {
          "composer/installers": "^1.2",
          "gotoandplay/gravityforms-composer-installer": "^2.3"
        }
      }
    }
  ],
  "require": {
    "php": ">=8.3",
    "composer/installers": "^1.2",
    "vlucas/phpdotenv": "^5.5",
    "oscarotero/env": "^2.1",
    "roots/bedrock-autoloader": "^1.0",
    "roots/bedrock-disallow-indexing": "^2.0",
    "roots/wordpress": "^6.5",
    "roots/wp-config": "^1.0.0",
    "roots/multisite-url-fixer": "^1.1",
    "wpengine/advanced-custom-fields-pro": "^6.3",
    "deliciousbrains-plugin/wp-migrate-db-pro": "^2.3",
    "wpackagist-plugin/autodescription": "^5.0",
    "wpackagist-plugin/better-image-sizes": "^3.4",
    "wpackagist-plugin/block-visibility": "^3.4",
    "wpackagist-plugin/duplicate-page": "^4.5",
    "wpackagist-plugin/loco-translate": "^2.6",
    "wpackagist-plugin/menus-for-block-theme": "^1.0",
    "wpackagist-plugin/redirection": "^5.1",
    "wpackagist-plugin/redis-cache": "^2.0",
    "wpackagist-plugin/shortpixel-image-optimiser": "^6.0",
    "wpackagist-plugin/wp-sentry-integration": "^8.2.0",
    "wpackagist-plugin/wp-mail-smtp": "^4.0",
    "wpackagist-plugin/wps-hide-login": "^1.8",
    "gravityforms/gravityforms": "*",
    "wp-media/wp-rocket": "^v3.15.10",
    "wpackagist-plugin/gravity-forms-zero-spam": "^1.4",
    "wpackagist-plugin/wp-crontrol": "^1.16",
    "wpackagist-plugin/transients-manager": "^2.0",
    "wpackagist-plugin/user-role-editor": "^4.64",
    "wpackagist-plugin/block-catalog": "^1.6",
    "wpackagist-plugin/duracelltomi-google-tag-manager": "^1.20"
  },
  "require-dev": {
    "phpro/grumphp": "^1.4.0",
    "squizlabs/php_codesniffer": "^3.7.1",
    "roave/security-advisories": "dev-latest",
    "friendsofphp/php-cs-fixer": "^3.13",
    "tracy/tracy": "^2.10"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "allow-plugins": {
      "composer/installers": true,
      "roots/wordpress-core-installer": true,
      "ffraenz/private-composer-installer": true,
      "phpro/grumphp": true,
      "gotoandplay/gravityforms-composer-installer": true,
      "php-http/discovery": true
    }
  },
  "autoload": {
    "exclude-from-classmap": [
      "/web/app/plugins/wp-rocket/inc/Dependencies/ActionScheduler",
      "/web/app/plugins/wp-rocket/inc/Engine"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "web/app/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "web/app/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "post-autoload-dump": [
      "@php -r \"if(file_exists('vendor/bin/grumphp')) {echo exec('vendor/bin/grumphp git:init');}\""
    ],
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "test": [
      "phpcs"
    ]
  }
}
